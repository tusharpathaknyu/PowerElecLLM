      "id": "L1_001",
      "prompt": "Design a buck converter: 12V input to 5V output, 2A load current",
      "solution": {
        "topology": "buck",
        "vin": 12.0,
        "vout": 5.0,
        "iout": 2.0,
        "power": 10.0,
        "f_sw": 200000,
        "duty_cycle": 0.417,
        "components": {
          "L": {"value": 47e-6, "unit": "H", "rating": "2.5A saturation"},
          "C_out": {"value": 100e-6, "unit": "F", "type": "ceramic or electrolytic"},
          "C_in": {"value": 22e-6, "unit": "F", "type": "ceramic"},
          "R_load": {"value": 2.5, "unit": "Ω"}
        },
        "design_equations": {
          "duty_cycle": "D = Vout/Vin = 5/12 = 0.417",
          "inductor": "L = Vout*(1-D)/(f_sw*ΔIL) = 5*(1-0.417)/(200e3*0.4) = 36.5µH → 47µH",
          "capacitor": "C = ΔIL/(8*f_sw*ΔVout) = 0.4/(8*200e3*0.025) = 10µF → 100µF"
        },
        "expected_results": {
          "vout_actual": 5.0,
          "ripple_mv": 25,
          "efficiency_est": 0.92
        }
      }
    },
